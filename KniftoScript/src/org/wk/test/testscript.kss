;Script zum Test des Lexers und des Compilers

float foo = 0
int max = 1000

func test(int i,float f)
	i = i + 1
	if(i == max+1)
		return
	end
	foo = foo + f
	test(i,f)
end

func returnTest(int i)
	if(i == 0)
		return 1
	end
	
	if(i != 0)
		return (i * returnTest(i-1))
	end
end

test(0,1.0)

foo = returnTest(4)

end.