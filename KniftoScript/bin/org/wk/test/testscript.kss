# Testscript for compiler

string thisIsAKlohbahhl = "föföfö";

out.println(thisIsAKlohbahhl);

public func onInit(int i)

	out.println(thisIsAKlohbahhl);

	Object nv = new org.wk.test.ConstrTest();
	
	nv.setv(nv);
	factorial(1);

	out.println(give("asdfasdf"));

	return factorial(i);
	
end

private func factorial(int i)

	if(i == 0)
		return 1;
	end
	
	return i*factorial(i-1);

end

thisIsAKlohbahhl = "inti: " + 1020;

private func give(string s)

	return new java.lang.String(s + 123.5);

end

